import Logo from "@components/logos/Logo/Logo.astro";
import { SONGS } from "@views/SongsView/config";
---

<header class={css.header}>
  <nav>
    <Link href={INDEX.href}><Logo /></Link>
    <Link href={SONGS.href}>{SONGS.display}</Link>
  </nav>
</header>
